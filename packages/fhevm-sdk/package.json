{
  "name": "@fhevm-template/fhevm-sdk",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK - Framework-agnostic SDK for Zama's Fully Homomorphic Encryption",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./adapters/react": {
      "import": "./dist/adapters/react.js",
      "types": "./dist/adapters/react.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fhe",
    "fhevm",
    "zama",
    "encryption",
    "homomorphic-encryption",
    "privacy",
    "blockchain",
    "ethereum",
    "sdk",
    "react",
    "nextjs",
    "framework-agnostic"
  ],
  "author": "FHEVM Template Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DeonReichert/fhevm-react-template.git",
    "directory": "packages/fhevm-sdk"
  },
  "peerDependencies": {
    "fhevmjs": "^0.5.0",
    "ethers": "^6.10.0",
    "react": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0"
  }
}
